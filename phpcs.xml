<?xml version="1.0"?>
<ruleset name="Swat">
  <arg name="colors"/>
  <arg name="tab-width" value="4"/>
  <arg name="extensions" value="php"/>
  <arg name="encoding" value="utf-8"/>
  <arg name="warning-severity" value="0"/>
  <arg value="p" /><!-- show progress -->

  <file>./Swat</file>
  <file>./SwatDB</file>
  <file>./SwatI18N</file>

  <rule ref="SilverorangeTransitionalPrettier"/>
</ruleset>
